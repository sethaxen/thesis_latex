@article{clancySecondharmonicPhaseDetermination2017,
  title = {Second-Harmonic Phase Determination by Real-Time in Situ Interferometry},
  author = {Clancy, Bason and Salafsky, Joshua},
  year = {2017},
  volume = {19},
