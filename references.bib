@article{smithEnhancingNMRDerived2020,
  title = {Enhancing {{NMR}} Derived Ensembles with Kinetics on Multiple Timescales},
  author = {Smith, Colin A. and Mazur, Adam and Rout, Ashok K. and Becker, Stefan and Lee, Donghan and {de Groot}, Bert L. and Griesinger, Christian},
  year = {2020},
